<?xml version="1.0" encoding="utf-8"?>

<!-- Pour plus d'informations sur l'utilisation de la transformation web.config, consultez http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <!--
      Dans l’exemple ci-dessous, la transformation "SetAttributes" remplacera  
      la valeur de l'attribut value de l'element add de appSettings correspondant à la clé "MySetting".
 
 <appSettings>
 
  <add key="MySetting" value="MyNewValue"  xdt:Transform="SetAttributes(value)" xdt:Locator="Match(key)"/>
 
 </appSettings>
 
  -->

</configuration>